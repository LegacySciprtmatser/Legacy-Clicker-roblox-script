if game.PlaceId == 6881150984 then

getgenv().autoTap = false;
getgenv().autoRebirth = false;
getgenv().buyEgg = false;
getgenv().autocraft = false;
    
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Legacy Clickers", "Sentinel")

  --Farming--
    local Main = Window:NewTab("Farming")
    local MainSection = Main:NewSection("Farming ")

    MainSection:NewToggle("Auto Tap", "Auto clicks for you",function(bool)
        getgenv().autoTap = bool
        print("Auto Tap is: ", bool)
        if bool then
            doTap();
        end
    end)

    local selectRebirth
    MainSection:NewDropdown("Select Rebirths", "Click to pick how much to auto rebirth", {"1 Rebirth","5 Rebirths","10 Rebirths","25 Rebirths","50 Rebirths","100 Rebirths","250 Rebirths","500 Rebirths","1k Rebirths","2k Rebirths","5k Rebirths","10k Rebirths","25k Rebirths","50k Rebirths","100k Rebirths", "250k Rebirths","500k Rebirths","1M Rebirths","2.5M Rebirths","5M Rebirths","7.5M Rebirths","10M Rebirths","15M Rebirths","20M Rebirths","35M Rebirths","50M Rebirths","75M Rebirths","100M Rebirths","125N Rebirths","150M Rebirths","175M Rebirths","200M Rebirths"}, function(value)
        selectRebirth = value
        print(value)
    end)

    MainSection:NewToggle("Auto Rebirth", "Toggles Auto rebirth", function(bool)
        getgenv().autoRebirth = bool
        print("Auto Rebirth is: ", bool)
        if bool and selectRebirth then
            autoRebirth(selectRebirth);
        end
    end)
    
    local selectEgg;
    MainSection:NewDropdown("Select Egg",  "Click to pick what egg to open",{"Common Egg","Rare Egg","Sand Egg","Gummy Egg","Sundae Egg","Cotton Candy Egg","Oceanic Egg","Deep Sea Egg","Fiery Egg","Hellfire Egg","Mushroom Egg","Moonshroom Egg","Spore Egg","Yolk Egg","Food Egg","Mistletoe Egg","Christmas Night Egg","Aurora Borealis Egg","Gingerbread Egg","Fruitcake Egg"}, function(value) --true/false, replaces the current title "Dropdown" with the option that t
    selectEgg = value;
    print(value)
end)

MainSection:NewToggle("Auto Hatch", "Open 1 egg",function(bool)
    getgenv().buyEgg = bool
    print("Auto hatch is: ", bool)
    if bool and selectEgg then
        buyEgg(selectEgg);
    end
end)

MainSection:NewToggle("Auto Hatch Triple -- need game pass --","Opens 3 egg",function(bool)
    getgenv().buyEgg = bool
    print("Auto hatch is: ", bool)
    if bool and selectEgg then
        buyEggtriple(selectEgg);
    end
end)



    -- PLAYER
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")

    PlayerSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end)

    PlayerSection:NewSlider("Jumppower", "Changes the jumppower", 250, 50, function(v)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
    end)

    --functions
function doTap()
    spawn(function()
        while getgenv().autoTap == true do
game:GetService("ReplicatedStorage").Shared.Knit.Services.remoteService.RE.clickRemote:FireServer()
wait(0)
       end
     end)
end
   
function autoRebirth()
        spawn(function()
            while getgenv().autoRebirth == true do
                local args = {
                    [1] = "Rebirth",
                    [2] = selectRebirth
                }
                game:GetService("ReplicatedStorage").Shared.Knit.Services.remoteService.RE.rebirthRemote:FireServer(unpack(args))
                wait(0)
                
                    end 
                end)
             end

             function buyEgg()
                spawn(function()
                    while getgenv().buyEgg == true do
            local args = {
                [1] = "Single", 
                [2] = selectEgg
            }
            game:GetService("ReplicatedStorage").Shared.Knit.Services.petService.RF.hatchEgg:InvokeServer(unpack(args))
                        wait()
                    end
                end)    
            end

            function buyEggtriple()
                spawn(function()
                    while getgenv().buyEgg == true do
            local args = {
                [1] = "Triple", 
                [2] = selectEgg
            }
            game:GetService("ReplicatedStorage").Shared.Knit.Services.petService.RF.hatchEgg:InvokeServer(unpack(args))
                        wait()
                    end
                end)    
            end

    elseif game.PlaceId == 3956818381 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Clickers Madness", "Sentinel")

    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")

    MainSection:NewToggle("Auto Swing", "Make your player autoswing", function(v)
        getgenv().autoswing = v
        while true do
            if not getgenv().autoswing then return end
            for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v:FindFirstChild("ninjitsuGain") then
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                    break
                end
            end
            local A_1 = "swingKatana"
            local Event = game:GetService("Players").LocalPlayer.ninjaEvent
            Event:FireServer(A_1)
            wait(0.1)
        end
    end)

    MainSection:NewToggle("Auto Sell", "Makes your player autosell", function(v)
        getgenv().autosell = v
        while true do
            if getgenv().autoswing == false then return end
            game:GetService("Workspace").sellAreaCircles["sellAreaCircle16"].circleInner.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            wait(0.1)
            game:GetService("Workspace").sellAreaCircles["sellAreaCircle16"].circleInner.CFrame = CFrame.new(0,0,0)
            wait(0.1)
        end
    end)

    MainSection:NewButton("Unlock all islands", "Unlocks all islands", function()
        local oldcframe = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        for _,v in pairs(game:GetService("Workspace").islandUnlockParts:GetChildren()) do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
            wait(0.1)
        end
        wait(0.1)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldcframe
    end)
    
    MainSection:NewToggle("Auto buy all swords", "Auto buys all swords", function(v)
        getgenv().buyswords = v
        while true do
            if not getgenv().buyswords then return end
            local A_1 = "buyAllSwords"
            local A_2 = "Inner Peace Island"
            local Event = game:GetService("Players").LocalPlayer.ninjaEvent
            Event:FireServer(A_1, A_2)
            wait(0.5)
        end
    end)

    MainSection:NewToggle("Auto buy all belts", "Auto buys all belts", function(v)
        getgenv().buybelts = v
        while true do
            if not getgenv().buybelts then return end
            local A_1 = "buyAllBelts"
            local A_2 = "Inner Peace Island"
            local Event = game:GetService("Players").LocalPlayer.ninjaEvent
            Event:FireServer(A_1, A_2)
            wait(0.5)
        end
    end)
end
